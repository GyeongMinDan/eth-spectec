;;
;; Unary operation oracle
;;

dec $un_op(unop, val) : val   hint(show %#(%)#%latex(""))

;;
;; Binary operation oracle
;;

dec $bin_op(binop, val, val) : val   hint(show %#(%, %))

;;
;; Cast oracle
;;

dec $cast_op(typ, val) : val   hint(show (%) %)

;;
;; Bitstring access oracle
;;

dec $bitacc_op(val, val, val) : val   hint(show %`[%`:%])
