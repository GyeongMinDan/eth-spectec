;;

def $int_to_bits(int) : bits
def $bits_to_int(bits) : int

;;
;; Unary operation oracle
;;

def $un_op(unop, val) : val   hint(show %#(%)#%latex(""))

;;
;; Binary operation oracle
;;

def $bin_op(binop, val, val) : val   hint(show %#(%, %))

;;
;; Cast oracle
;;

def $cast_op(typ, val) : val   hint(show (%) %)

;;
;; Bitstring access oracle
;;

def $bitacc_op(val, val, val) : val   hint(show %`[%`:%])
